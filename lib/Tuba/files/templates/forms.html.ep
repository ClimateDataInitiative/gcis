% layout 'default';

<h3>Forms</h3>
<ul>
% for my $form (sort @$forms) {
% my $name = $form; $name =~ s/create_form_//;
<li>
    %= link_to $form => { report_identifier => $report_identifier } => begin
        New <%= $name %>
    %= end
</li>
% }
<li>
%= link_to import_spreadsheet => begin
Import spreadsheet
%= end
</li>
</ul>
