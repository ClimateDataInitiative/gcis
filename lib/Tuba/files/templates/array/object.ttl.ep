% layout 'default', namespaces => [qw/gcis dcterms/];

<<%= current_resource %>>
   a gcis:Array;
   dcterms:identifier "<%= $array->identifier %>";

% for my $table ($array->tables) {
   gcis:isArrayOf <<%= obj_uri_for($table)->to_abs %>>.
% }   

% for my $table ($array->tables) {
<<%= obj_uri_for($table)->to_abs %>> a gcis:Table .
% }
