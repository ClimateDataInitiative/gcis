% layout 'default';

debug
<pre>
% for my $table (keys %{ orm() }) {
% my $mng = orm->{$table}{mng};
% my $obj = orm->{$table}{obj};
<b><%= $table %></b>
columns :  <%= join ',', $obj->meta->columns; %>
relationships :
 <% for my $rel ($obj->meta->relationships) { %> <%= $rel->name %> <% } %>
% }
</pre>
